{
    "name": "job-tracker",
    "version": "0.1.0",
    "proxy": "http://localhost:3001",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@material-ui/core": "^4.12.4",
        "@mui/icons-material": "^5.6.2",
        "@mui/material": "^5.6.2",
        "@types/body-parser": "^1.19.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/material-ui": "^0.21.12",
        "@types/node": "^17.0.25",
        "@types/passport-jwt": "^3.0.6",
        "@types/react": "^18.0.5",
        "@types/react-dom": "^18.0.1",
        "@types/supertest": "^2.0.12",
        "axios": "^0.26.1",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.0",
        "bootstrap": "^5.1.3",
        "cjs": "^0.0.11",
        "concurrently": "^7.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "cra-template-typescript": "1.1.3",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-mysql-session": "^2.1.7",
        "express-session": "^1.17.2",
        "forever": "^4.0.3",
        "jest": "^27.5.1",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.3.3",
        "nodemon": "^2.0.15",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "react": "^18.0.0",
        "react-bootstrap": "^2.2.3",
        "react-dom": "^18.0.0",
        "react-router-bootstrap": "^0.26.1",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.0",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.4",
        "ts-loader": "^9.2.8",
        "typescript": "^4.6.3",
        "wait-on": "^6.0.1",
        "webpack-cli": "^4.9.2",
        "webpack-node-externals": "^3.0.0"
    },
    "scripts": {
        "database:local": "node ./config/setupScript.mjs LOCAL",
        "database:azure": "node ./config/setupScript.mjs AZURE",
        "test:frontend": "react-scripts test",
        "test:backend": "jest --selectProjects backend --forceExit #--detectOpenHandles",
        "eject": "react-scripts eject",
        "dev": "concurrently -p name -c \"yellow,magenta,blue\" -n \"webpack-server,nodemon-server,CRA\" \"npm run dev:server:webpack\" \"npm run dev:server:nodemon\" \"npm run dev:client\"",
        "dev:client": "wait-on -l tcp:3001 && react-scripts start",
        "dev:server": "concurrently -p name -c \"yellow,magenta\" -n \"webpack-server,nodemon-server\" \"npm run dev:server:webpack\" \"npm run dev:server:nodemon\"",
        "dev:server:webpack": "webpack --config webpack.config.server.js --watch",
        "dev:server:nodemon": "rm -f build/server.js && wait-on -l build/server.js && nodemon build/server.js",
        "build": "npm run build:client && npm run build:server",
        "build:client": "react-scripts build",
        "build:server": "webpack --config webpack.config.server.js"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/express-session": "^1.17.4",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/passport": "^1.0.7",
        "@types/passport-local": "^1.0.34",
        "@types/react-router-bootstrap": "^0.24.5"
    }
}
